<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.statusbar.phone.StatusBarWindowView
  android:fitsSystemWindows="true"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    
  <com.android.systemui.statusbar.BackDropView
    android:id="@*com.android.systemui:id/backdrop"
    android:visibility="gone"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
      <ImageView
        android:id="@*com.android.systemui:id/backdrop_back"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="centerCrop" />
      <ImageView
        android:id="@*com.android.systemui:id/backdrop_front"
        android:visibility="invisible"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="centerCrop" />
  </com.android.systemui.statusbar.BackDropView>
  <ImageView
    android:layout_gravity="center|top"
    android:id="@*com.android.systemui:id/screendecor_up"
    android:visibility="gone"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="@*com.android.systemui:dimen/status_bar_header_height_keyguard"
    android:scaleType="center" />
  <com.android.systemui.statusbar.ScrimView
    android:id="@*com.android.systemui:id/scrim_behind" 
    android:visibility="invisible"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:importantForAccessibility="no" />
  <FrameLayout
    android:id="@*com.android.systemui:id/status_bar_container"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" />
  <ViewStub
    android:id="@*com.android.systemui:id/fullscreen_user_switcher_stub"
    android:layout="@*com.android.systemui:layout/car_fullscreen_user_switcher"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" />
  <include
    android:visibility="invisible"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    layout="@*com.android.systemui:layout/status_bar_expanded" />
  <include layout="@*com.android.systemui:layout/brightness_mirror" />
  <com.android.systemui.statusbar.ScrimView
    android:id="@*com.android.systemui:id/scrim_in_front"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:importantForAccessibility="no" />
  <FrameLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <include
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      layout="@*com.android.systemui:layout/op_warp_charging_animation_view" />
    <include
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      layout="@*com.android.systemui:layout/opfacelock_light_view" />
    <include
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      layout="@*com.android.systemui:layout/keyguard_prevent_mode" />
  </FrameLayout>
</com.android.systemui.statusbar.phone.StatusBarWindowView>